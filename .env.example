# TriCrawl environment variables (example)
DISCORD_WEBHOOK_URL=

# Supabase (Required for DB & Dedup)
SUPABASE_URL=
SUPABASE_KEY=

# Superset (Dashboard)
SUPERSET_CLOUD_URL=
# SUPERSET_LOCAL_URL=http://127.0.0.1:8088
TRICRAWL_SUPERSET_MODE=cloud   # cloud | local
SUPERSET_DEFAULT_PATH=/superset/welcome/

# Tor proxy
TOR_PROXY_HOST=127.0.0.1
TOR_PROXY_PORT=9050
TOR_PROXY_SCHEME=socks5h

# Crawl tuning
CONCURRENT_REQUESTS=2
CRAWL_DELAY=5
VERIFY_SSL=true

# Dedup cache
DEDUP_MAX_ENTRIES=20000
DEDUP_MAX_DAYS=30
DEDUP_PRUNE_UNSEEN=false

# Notifications
NOTIFY_ON_NO_NEW_DATA=true

# CLI display
TRICRAWL_PLAIN=false
TRICRAWL_NO_COLOR=false
